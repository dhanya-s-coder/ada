#include <stdio.h>

int mergeAndCount(int arr[], int left, int mid, int right, int k) {
    int count = 0;
    int j = mid + 1;

    // Count pairs satisfying A[i] > k * A[j]
    for (int i = left; i <= mid; i++) {
        while (j <= right && arr[i] > k * arr[j]) {
            j++;
        }
        count += (j - (mid + 1));
    }

    // Merge step
    int n1 = mid - left + 1;
    int n2 = right - mid;
    int L[n1], R[n2];

    for (int i = 0; i < n1; i++)
        L[i] = arr[left + i];
    for (int i = 0; i < n2; i++)
        R[i] = arr[mid + 1 + i];

    int i = 0, r = 0, t = left;

    while (i < n1 && r < n2) {
        if (L[i] <= R[r])
            arr[t++] = L[i++];
        else
            arr[t++] = R[r++];
    }

    while (i < n1)
        arr[t++] = L[i++];
    while (r < n2)
        arr[t++] = R[r++];

    return count;
}

int countPairsUtil(int arr[], int left, int right, int k) {
    if (left >= right)
        return 0;

    int mid = (left + right) / 2;
    int count = 0;

    count += countPairsUtil(arr, left, mid, k);
    count += countPairsUtil(arr, mid + 1, right, k);
    count += mergeAndCount(arr, left, mid, right, k);

    return count;
}

int count_pairs(int arr[], int n, int k) {
    return countPairsUtil(arr, 0, n - 1, k);
}

int main() {
    int n, k;
    printf("Enter the value of n (<= 100): ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d integers: ", n);
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter the integer value k: ");
    scanf("%d", &k);

    int result = count_pairs(arr, n, k);
    printf("Number of pairs: %d\n", result);

    return 0;
}
