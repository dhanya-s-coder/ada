#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    int x;
    scanf("%d", &x);
    
    int a[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    int sum = 0, start = 0, end = -1;

    for (int i = 0; i < n; i++) {
        sum += a[i];

        while (sum > x && start <= i) {  // shrink window until sum <= x
            sum -= a[start];
            start++;
        }

        if (sum == x) {
            end = i;
            break;
        }
    }

    if (end != -1)
        printf("Subarray found from index %d to %d\n", start, end);
    else
        printf("No subarray found with sum %d\n", x);

    return 0;
}

