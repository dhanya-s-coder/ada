#include <stdio.h>

// Recursive function to find nth Fibonacci number
int fibonacci(int n) {
    if (n == 0)
        return 1;
    else if (n == 1)
        return 1;
    else if(n==2){
        return 2;
    }
    else
        return fibonacci(n - 1) + fibonacci(n - 2)+fibonacci(n-3);
}



